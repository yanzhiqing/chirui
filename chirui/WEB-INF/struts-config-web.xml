<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="infomationForm" type="crjd.model.admin.sommanage.infomation.SomInfomationForm" />
		<form-bean name="shoolfellowAppForm" type="crjd.model.admin.sommanage.schoolfellow.SchoolfellowForm" />
		<form-bean name="teacherForm" type="crjd.model.admin.sommanage.teachers.SomTeacherForm" />
		<form-bean name="leaderFormApp"  type="crjd.model.admin.sommanage.leader.LeaderForm" />
		<form-bean name="onLineBmFormApp"  type="crjd.model.admin.sommanage.onLineBm.OnLineBmForm"/>
		<form-bean name="deanBoxForm" type="crjd.model.somweb.deanBox.DeanBoxForm"/>
	</form-beans>
	<action-mappings>
		<!-- 前台新闻阅读　李永祥　2009.06.24  start -->
		<action name="infomationForm" path="/infomationAction"
			scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="success"       path="/somweb/lookInfomation.jsp" />
			<forward name="addsuccess"    path="/infomationAction.do?actionType=0" />
			<forward name="detail"        path="/somweb/look.jsp" />
			<forward name="news"          path="/somweb/newdetail.jsp" />
			<forward name="preUPdate"     path="/somweb/lookInfomation.jsp" />
			<forward name="updatesuccess" path="/infomationAction.do?actionType=0" />
			<forward name="delsuccess"    path="/infomationAction.do?actionType=0" />
			<forward name="searchSuccess" path="/somweb/lookInfomation.jsp" />
			<forward name="search" path="/somweb/search.jsp" />
			<forward name="zt_succes" path="/somweb/ztNews/ztindex.jsp" />
			<forward name="zt_detail" path="/somweb/ztNews/ztlook.jsp" />
			<forward name="search_detail" path="/somweb/searchlook.jsp" />
			
		</action>
		<!-- 前台新闻阅读　李永祥　2009.06.24  end -->
		<!-- 前台杰出校友　李永祥　2009.07.01  start -->
		<action name="shoolfellowAppForm" path="/schoolfellowAppAction"
			scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="success"       path="/somweb/studentsWork/schoolfellow.jsp" />
			<forward name="addsuccess"    path="/schoolfellowAppAction.do?actionType=0" />
			<forward name="detail"        path="/somweb/studentsWork/schoolfellowLook.jsp" />
			<forward name="preUPdate"     path="/somweb/studentsWork/schoolfellow.jsp" />
			<forward name="updatesuccess" path="/schoolfellowAppAction.do?actionType=0" />
			<forward name="delsuccess"    path="/schoolfellowAppAction.do?actionType=0" />
			<forward name="searchSuccess" path="/somweb/studentsWork/schoolfellow.jsp" />
		</action>
		<!--  前台杰出校友  李永祥　2009.07.01  end -->
		<!-- 前台导师风采模块　李永祥　2009.07.02  start -->
		<action name="teacherForm" path="/teacherAction"
			scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="success"       path="/somweb/teacherTeam/teacherTeam.jsp" />
			<forward name="addsuccess"    path="/teacherAction?actionType=0" />
			<forward name="detail"        path="/somweb/teacherTeam/teacherTeamLook.jsp" />
			<forward name="preUPdate"     path="/somweb/teacherTeam/teacherTeamLook.jsp" />
			<forward name="updatesuccess" path="/teacherAction?actionType=0" />
			<forward name="delsuccess"    path="/teacherAction?actionType=0" />
			<forward name="searchSuccess" path="/somweb/teacherTeam/teacherTeam.jsp"/>
		</action>
		<!--  前台导师风采模块  李永祥　2009.07.02  end -->
		<!--  学院领导　　 李永祥　2009.06.09 start -->
		<action path="/leaderActionApp" scope="request" name="leaderFormApp"
			type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="success"       path="/somweb/leader/leader.jsp" />
			<forward name="updatesuccess" path="/leaderActionApp.do?actionType=0" />
			<forward name="detail"        path="/somweb/leader/leaderLook.jsp" />
			<forward name="preUPdate"     path="/somweb/leader/leaderLook.jsp" />
		</action>
		<!--  学院领导　　 李永祥　2009.06.09  end -->
		<!-- 首页  王强　2009.06.29  start -->
		<action name="somInfomationForm" path="/somInfomationActionapp"
			scope="request"
			type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="somweb" path="/somweb/index.jsp" />
		</action>
		<!-- 首页  王强　2009.06.29  end -->
		<!--  学院领导　　 李永祥　2009.09.16 start -->
		<action path="/onLineBmActionApp" scope="request" name="onLineBmFormApp"
			type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="addsuccess"    path="/somweb/onlinebm/onLineBM.jsp" />
			<forward name="success"       path="/somweb/onlinebm/onLineBM.jsp"/>
			<forward name="updatesuccess" path="/onLineBmActionApp.do?actionType=0"/>
			<forward name="detail"        path="/somweb/onlinebm/onLineBM.jsp"/>
			<forward name="preUPdate"     path="/somweb/onlinebm/onLineBM.jsp"/>
		</action>
		<!--  学院领导　　 李永祥　2009.09.16  end -->
		<!-- 前台 领导信箱 hewei 2009.09.16 -->
		<action name="deanBoxForm" path="/webDeanBoxAction" scope="request" type="org.springframework.web.struts.DelegatingActionProxy">
		<forward name="success" path="/somweb/deanBox/replyList.jsp"></forward>
		<forward name="addsuccess" path="/webDeanBoxAction.do?actionType=0"></forward>
		<forward name="detail" path="/somweb/deanBox/yuanzhang_huih.jsp"></forward>
		<forward name="searchSuccess" path="/somweb/deanBox/replyList.jsp"></forward>
		</action>
		<!-- 前台 领导信箱 hewei 2009.09.16 -->
	</action-mappings>
</struts-config>
<class
    name="crjd.model.admin.system.menu.MenuInfo"
    table="TB_M_MENUINFO"
>



  <id
        name="menuRowId"
        type="java.lang.Integer"
        column="MENU_ROW_ID"
    >
        <generator class="increment" />
    </id>

    <property
        name="menuId"
        type="java.lang.String"
        column="MENU_ID"
    />
    <property
        name="menuName"
        type="java.lang.String"
        column="MENU_NAME"
    />
    <property
        name="menuUpId"
        type="java.lang.String"
        column="MENU_UP_ID"
    />
    <property
        name="menuUrl"
        type="java.lang.String"
        column="MENU_URL"
    />
    <property
        name="menuRemark"
        type="java.lang.String"
        column="MENU_REMARK"
    />
    <property
        name="menuCol1"
        type="java.lang.String"
        column="MENU_COL1"
    />
    <property
        name="menuCol2"
        type="java.lang.String"
        column="MENU_COL2"
    />
    <property
        name="menuLuser"
        type="java.lang.String"
        column="MENU_LUSER"
    />
     <set name="menupops"
        	cascade="all"
               inverse="true"
               lazy="true"
        	table="TB_M_MENU_POPE"
        	>
            <key column="MENU_ID"/>
            <one-to-many class="crjd.model.admin.system.menupop.MenuPope"/>
      </set>


     <!--set name="roleMenuPopeLists"
        	cascade="all"

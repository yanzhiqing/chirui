<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<!--
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class
    name="crjd.model.admin.system.role.RoleInfo"
    table="TB_M_ROLE"
>

   <id
        name="roleRowId"
        type="java.lang.Integer"
        column="ROLE_ROW_ID"
    >
        <generator class="increment" />
    </id>



    <property
        name="roleId"
        type="java.lang.String"
        column="ROLE_ID"

    />
    <property
        name="roleName"
        type="java.lang.String"
        column="ROLE_NAME"

    />
    <property
        name="roleRemark"
        type="java.lang.String"
        column="ROLE_REMARK"

    />
    <property
        name="roleCol1"
        type="java.lang.String"
        column="ROLE_COL1"
    />
    <property
        name="roleCol2"
        type="java.lang.String"
        column="ROLE_COL2"

    />
    <property
        name="roleLuser"
        type="java.lang.String"
        column="ROLE_LUSER"
    />


    <set name="userRoleList"
         cascade="all"
         inverse="true"
         lazy="true"
    >
         <key column="ROLE_ID"/>
         <one-to-many class="crjd.model.admin.system.userrole.UserRole"/>
    </set>


    <set name="roleMenuPopList"
         cascade="all"
         inverse="true"
         lazy="true"
    >
         <key column="ROLE_ID"/>
         <one-to-many class="crjd.model.admin.system.role.RoleMenuPope"/>
    </set>
</class>
</hibernate-mapping>
